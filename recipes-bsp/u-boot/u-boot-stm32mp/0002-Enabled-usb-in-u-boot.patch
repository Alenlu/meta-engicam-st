+++ b/arch/arm/dts/stm32mp157a-ugea512-mx.dts
@@ -1100,6 +1100,7 @@
 
 	/* USER CODE BEGIN usbphyc_port0 */
 	st,phy-tuning = <&usb_phy_tuning>;
+	phy-supply = <&vdd_usb>;
 	/* USER CODE END usbphyc_port0 */
 };
 
@@ -1108,6 +1109,7 @@
 
 	/* USER CODE BEGIN usbphyc_port1 */
 	st,phy-tuning = <&usb_phy_tuning>;
+	phy-supply = <&vdd_usb>;
 	/* USER CODE END usbphyc_port1 */
 };
 
