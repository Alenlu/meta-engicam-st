 				partition@600000 {
-					label = "fip2";
+					label = "fip-a2";
 					reg = <0x600000 0x400000>;
 				};
 
 				partition@A00000 {
+					label = "fip-b1";
+					reg = <0xA00000 0x400000>;
+				};
+
+				partition@E00000 {
+					label = "fip-b2";
+					reg = <0xE00000 0x400000>;
+				};
+
+				partition@1200000 {
 					label = "UBI";
-					reg = <0xA00000 0x1F600000>;
+					reg = <0x1200000 0x1EE00000>;
 				};
 		};
 	};	
