@@ -740,23 +740,48 @@
 				#size-cells = <1>;
 
 				partition@0 {
-					label = "fsbl";
-					reg = <0x00000000 0x200000>;
+					label = "fsbl1";
+					reg = <0x00000000 0x80000>;
+				};
